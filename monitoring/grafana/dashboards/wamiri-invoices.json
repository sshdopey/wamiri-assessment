            "lineWidth": 2,
            "fillOpacity": 15,
            "gradientMode": "scheme",
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": true,
            "thresholdsStyle": {
              "mode": "area"
            }
          },
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 90
              },
              {
                "color": "green",
                "value": 99
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byRegexp",
              "options": "Breaches.*"
