            "gradientMode": "scheme",
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": true,
            "thresholdsStyle": {
              "mode": "line"
            }
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 15
              },
              {
                "color": "red",
                "value": 30
              }
            ]
          }
        }
      },
      "options": {
        "tooltip": {
